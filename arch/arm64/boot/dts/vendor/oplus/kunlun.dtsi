/* #ifdef OPLUS_ARCH_EXTENDS */
/* Nan.Zhong@MULTIMEDIA.AUDIODRIVER.MACHINE, 2020/08/28, Add for oplus extend aduio config */
#include "kunlun-lito-audio-overlay.dtsi"
/* #endif OPLUS_ARCH_EXTENDS */

&soc {
    //#ifdef OPLUS_FEATURE_TP_BASIC
    /* Qicai.Gu@BSP.TP, 2020/07/30, Add for sim detect */
	oplus_sim_detect {
		compatible = "oplus, sim_detect";
		Hw,sim_det = <&tlmm 82 0>;
	};
    //#endif /* OPLUS_FEATURE_TP_BASIC */
    //#ifdef OPLUS_FEATURE_DEVICE_LOG
    /* ZhuYan@Network.RF.10612, 2020/10/28, enlarge modem diag buff size 15M */
	qcom,memshare {
		compatible = "qcom,memshare";

		qcom,client_1 {
			compatible = "qcom,memshare-peripheral";
			qcom,peripheral-size = <0x0>;
			qcom,client-id = <0>;
			qcom,allocate-boot-time;
			label = "modem";
		};

		qcom,client_2 {
			compatible = "qcom,memshare-peripheral";
			qcom,peripheral-size = <0x0>;
			qcom,client-id = <2>;
			label = "modem";
		};

		mem_client_3_size: qcom,client_3 {
			compatible = "qcom,memshare-peripheral";
			qcom,peripheral-size = <0xF00000>;
			qcom,client-id = <1>;
			qcom,allocate-on-request;
			label = "modem";
		};
	};
    //#endif /* OPLUS_FEATURE_DEVICE_LOG */
};

//#ifdef OPLUS_BUG_STABALITY
/* Zhaoxinyu@display.driver. 2020/08/20 */
&L5P {
        reg =  /bits/ 16 <0x4400>;
        regulator-min-microvolt = <1500000>;
        regulator-max-microvolt = <1500000>;
        //qcom,min-dropout-voltage = <300000>;
        regulator-boot-on;
};
//#endif /*OPLUS_BUG_STABALITY*/
